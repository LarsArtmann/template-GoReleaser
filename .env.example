# GoReleaser Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# GitHub Configuration
GITHUB_OWNER=your-github-username
GITHUB_REPO=your-repo-name
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxx

# Docker Configuration
DOCKER_USERNAME=your-docker-username
DOCKER_TOKEN=dckr_pat_xxxxxxxxxxxxxxxxxxxx

# Project Information
PROJECT_DESCRIPTION="Your amazing project description"
VENDOR_NAME="Your Company Name"
MAINTAINER_NAME="Your Name"
MAINTAINER_EMAIL="your.email@example.com"

# Homebrew Tap (Pro)
HOMEBREW_TAP_GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxx

# Scoop Bucket (Pro)
SCOOP_GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxx

# AUR (Pro)
AUR_KEY=~/.ssh/aur

# Package Publishing (Pro)
FURY_TOKEN=your-fury-token
FURY_ACCOUNT=your-fury-account

# Cloud Storage (Pro)
S3_BUCKET=your-s3-bucket
AWS_ACCESS_KEY_ID=AKIAXXXXXXXXXXXXXX
AWS_SECRET_ACCESS_KEY=xxxxxxxxxxxxxxxxxx

AZURE_STORAGE_ACCOUNT=yourstorageaccount
AZURE_STORAGE_CONTAINER=yourcontainer
AZURE_STORAGE_KEY=xxxxxxxxxxxxxxxxxxxx

GCS_BUCKET=your-gcs-bucket
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Artifactory (Pro)
ARTIFACTORY_HOST=your-artifactory.example.com
ARTIFACTORY_REPO=your-repo
ARTIFACTORY_USERNAME=your-username
ARTIFACTORY_PASSWORD=your-password

# Notifications (Pro)
DISCORD_WEBHOOK_ID=xxxxxxxxxxxxxxxxxxxx
DISCORD_WEBHOOK_TOKEN=xxxxxxxxxxxxxxxxxxxx

SLACK_WEBHOOK_URL=https://hooks.slack.com/services/xxx/xxx/xxx

TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/xxx

SMTP_FROM=noreply@example.com
SMTP_TO=team@example.com
SMTP_USERNAME=smtp-username
SMTP_PASSWORD=smtp-password

WEBHOOK_URL=https://your-webhook.example.com/releases
WEBHOOK_TOKEN="Bearer xxxxxxxxxxxxxxxxxxxx"

# Signing (Pro)
COSIGN_PRIVATE_KEY=/path/to/cosign.key
COSIGN_PASSWORD=your-cosign-password

# GitLab (Alternative to GitHub)
GITLAB_TOKEN=glpat-xxxxxxxxxxxxxxxxxxxx

# Gitea (Alternative to GitHub)
GITEA_TOKEN=xxxxxxxxxxxxxxxxxxxx